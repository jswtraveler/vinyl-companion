# Environment Variables for Vinyl Collection App
# Copy this file to .env and fill in your actual values

# Discogs API Token (for album_metadata_test.py)
# Get your token at: https://www.discogs.com/settings/developers
DISCOGS_TOKEN=your_discogs_token_here

# Google Gemini API Key (for AI mood analysis)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration (for cloud database)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# SerpAPI Key (for reverse image search)
VITE_SERPAPI_KEY=your_serpapi_key_here

# Last.fm API Configuration (for music recommendations)
# Get your API key from: https://www.last.fm/api/account/create
VITE_LASTFM_API_KEY=your_lastfm_api_key_here

# Optional: Enable/disable external recommendation features
VITE_ENABLE_EXTERNAL_RECOMMENDATIONS=true

# Cache settings (optional - defaults will be used if not set)
VITE_RECOMMENDATION_CACHE_HOURS=24
VITE_EXTERNAL_DATA_CACHE_HOURS=24

# API rate limiting (optional - defaults will be used if not set)
VITE_LASTFM_REQUEST_DELAY_MS=1000
VITE_MAX_SIMILAR_ARTISTS=20
VITE_MAX_ALBUMS_PER_TAG=30