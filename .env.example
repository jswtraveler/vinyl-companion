# Environment Variables for Vinyl Collection App
# Copy this file to .env and fill in your actual values

# Discogs API Token (for album_metadata_test.py)
# Get your token at: https://www.discogs.com/settings/developers
DISCOGS_TOKEN=your_discogs_token_here

# Google Gemini API Key (for AI mood analysis)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration (for cloud database)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# SerpAPI Key (for reverse image search)
VITE_SERPAPI_KEY=your_serpapi_key_here